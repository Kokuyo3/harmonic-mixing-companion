{
  "configuration": {
    "status": "error",
    "name": "LogToConsole",
    "packages": "dev.cstang.harmonicmixingcompanion",
    "ThresholdFilter": {
      "level": "debug"
    },
    "appenders": {
      "Console": {
        "name": "STDOUT",
        "PatternLayout": {
          "pattern": "[%-5level] %d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %c{1} - %msg%n"
        }
      }
    },
    "loggers": {
      "root": {
        "level": "debug",
        "AppenderRef": {
          "ref": "STDOUT"
        }
      }
    }
  }
}